set incsearch
set wildmenu wildmode=list:full
set cursorline
set laststatus=2
set statusline=%F%m%r%h%w\ [FORMAT=%{&ff}]\ [TYPE=%Y]\ [ASCII=\%03.3b]\ [HEX=\%02.2B]\ [POS=%04l,%04v][%p%%]\ [LEN=%L]
" for solarrized
let g:solarized_termcolors=256
set number
syntax enable
set background="dark" 
colorscheme solarized
